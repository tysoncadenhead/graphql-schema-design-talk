{
  "name": "graphql-talk",
  "version": "1.0.0",
  "description": "A GraphQL example",
  "scripts": {
    "test": "jest",
    "start": "serverless offline start"
  },
  "dependencies": {
    "apollo-server-lambda": "2.14.2",
    "cors": "2.8.5",
    "graphql": "14.3.0",
    "graphql-tag": "2.10.1",
    "graphql-tools": "4.0.4"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.23",
    "@types/jest": "24.0.11",
    "@types/node": "11.12.0",
    "babel-jest": "24.5.0",
    "jest": "24.5.0",
    "jest-transform-graphql": "2.1.0",
    "serverless": "1.40.0",
    "serverless-http": "1.9.1",
    "serverless-offline": "5.0.1",
    "serverless-plugin-typescript": "1.1.5",
    "serverless-webpack": "5.2.0",
    "text-loader": "0.0.1",
    "ts-jest": "24.0.0",
    "ts-loader": "5.3.3",
    "typescript": "3.3.4000",
    "webpack": "4.29.6",
    "webpack-node-externals": "1.7.2"
  },
  "author": "Tyson Cadenhead",
  "license": "MIT",
  "jest": {
